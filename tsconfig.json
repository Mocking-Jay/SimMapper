{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib":[
      "dom",
      "es2015",
      "es5",
      "es6"
    ],
    "downlevelIteration": true,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "dist"
  },
  "include":[
    "src/**/*"
   ],
    
  "exclude": [
    ".git",
    "node_modules" 
  ],
  "files":[
    "src/app.ts",
    "src/main.ts"
//    "src/simmapper.ts"
  ]
}
